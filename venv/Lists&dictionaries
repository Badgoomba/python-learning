# Lists
fruits = ["apple", "banana", "cherry"]
fruits.append("orange")
print(fruits[3]) # First Item
# print(fruits[0]) apple
# print(fruits[1]) banana
# print(fruits[2]) cherry
# print(fruits[3]) orange

# Dictionaries
person = {
    "name": "Mike",
    "age": 28,
    "skills": ["Python", "Git", "SQL", "Hardware Support", "Funny" ]
}
print(person["name"])
print(person["age"])
print(person["skills"])